<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-${org.springframework.base.version}.xsd">


    <!-- Commons initialization -->
    <import resource="classpath:config/applicationContext-commons.xml" />
    <!-- Logger initialization -->
    <import resource="classpath:config/applicationContext-logger.xml" />
    <!-- JSON initialization -->
    <import resource="classpath:config/applicationContext-json.xml" />
    <!-- Model initialization -->
    <import resource="classpath:config/applicationContext-model.xml" />
    <!-- Transaction management definition -->
    <import resource="classpath:config/applicationContext-transaction.xml" />
    <!-- Persistence initialization -->
    <import resource="classpath:config/applicationContext-persistence.xml" />
    <!-- JMS definition -->
    <import resource="classpath:config/applicationContext-jms.xml" />

    <!-- Client definition (used for remote queue empty wait) -->
    <import resource="classpath:config/applicationContext-clients.xml" />



    <!-- Connect initialization beans  -->
    <import resource="classpath:config/applicationContext-connect-initialization-beans.xml"/>
    <!-- Connect identification beans  -->
    <import resource="classpath:config/applicationContext-connect-identification-beans.xml"/>
    <!-- Connect continuity beans  -->
    <import resource="classpath:config/continuity/applicationContext-connect-continuity-beans.xml"/>
    <!-- Connect orchestration beans  -->
    <import resource="classpath:config/applicationContext-connect-orchestration-beans.xml"/>
    <import resource="classpath:config/applicationContext-connect-orchestration-camel.xml"/>
    <import resource="classpath:config/resources/applicationContext-resources-jms.xml"/>
    <!-- Connect services beans  -->
    <import resource="classpath:config/applicationContext-connect-services-beans.xml"/>
</beans>